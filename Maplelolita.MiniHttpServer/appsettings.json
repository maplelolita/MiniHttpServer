{
  "urls": "http://+:5000",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "UseDirectoryBrowser": true,
  "UsePathFilter": true,
  "PathFilter": [
    "^/\\.[^/]+",
    ".*(/|^)\\.[^/]+"
  ],
  "UseBasicAuth": true,
  "BasicAuth": {
    "Username": "admin",
    "Password": "admin"
  }
}
